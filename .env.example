# .env.example - Example environment configuration for homelab
# Copy this file to .env and fill in your values

# User and Group IDs (auto-generated by setup.sh)
PUID=1000
PGID=1000
RENDER_GID=109

# Timezone (e.g., America/New_York, Europe/London, Africa/Lagos)
TZ=Etc/UTC

# n8n Credentials
N8N_USER=admin
N8N_PASS=changeme_strong_password_here

# Samba Credentials
SAMBA_USER=your_username
SAMBA_PASS=changeme_strong_password_here

# Ollama Model (space-separated list)
OLLAMA_MODEL=llama3.2:1b llama3.2:3b qwen2.5-coder:3b

# Plex Claim Token (get from https://www.plex.tv/claim/ - valid for 4 minutes)
PLEX_CLAIM=

# Grafana Admin Password (if using monitoring stack)
GRAFANA_ADMIN_PASSWORD=changeme_strong_password_here

# Antigravity VNC Password (simple 8-char alphanumeric)
ANTIGRAVITY_VNC_PASSWORD=homelab1

# OpenClaw Gateway Token (auto-generated by setup.sh)
OPENCLAW_TOKEN=changeme_generate_random_hex
